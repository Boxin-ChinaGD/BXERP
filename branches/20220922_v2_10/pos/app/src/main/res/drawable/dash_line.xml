<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">

    <!--虚线，线宽为dashWith，线之间空隙dashGap，dashGap=0dp时，是实线 -->
    <stroke
        android:width="1dp"
        android:color="#828282"
        android:dashGap="6dp"
        android:dashWidth="8dp"/>
</shape>