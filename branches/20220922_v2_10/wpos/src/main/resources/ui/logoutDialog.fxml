<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane prefHeight="352.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="wpos.allController.LogoutDialogViewController">
    <center>
        <FlowPane orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="24.0" left="24.0" right="24.0" top="24.0"/>
            </padding>
            <children>
                <Label text="提示" textFill="#f44336">
                    <font>
                        <Font name="System Bold" size="56.0"/>
                    </font>
                </Label>
                <Label text="确认交班？" textFill="#263238">
                    <font>
                        <Font size="40.0"/>
                    </font>
                    <FlowPane.margin>
                        <Insets top="24.0"/>
                    </FlowPane.margin>
                </Label>
            </children>
        </FlowPane>
    </center>
    <bottom>
        <FlowPane alignment="CENTER_RIGHT" prefHeight="136.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="cancel" onMouseClicked="#cancel_click" id="blackButton" mnemonicParsing="false" prefHeight="88.0" prefWidth="196.0"
                        stylesheets="@../css/buttonBg.css" text="取消" textFill="WHITE">
                    <font>
                        <Font size="40.0"/>
                    </font>
                    <FlowPane.margin>
                        <Insets right="16.0"/>
                    </FlowPane.margin>
                </Button>
                <Button fx:id="sure" onMouseClicked="#sure_click" id="blueButton" mnemonicParsing="false" prefHeight="88.0" prefWidth="196.0"
                        stylesheets="@../css/buttonBg.css" text="确定" textFill="WHITE">
                    <font>
                        <Font size="40.0"/>
                    </font>
                    <FlowPane.margin>
                        <Insets right="24.0"/>
                    </FlowPane.margin>
                </Button>
            </children>
        </FlowPane>
    </bottom>
</BorderPane>
